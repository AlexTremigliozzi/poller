<p id="notice"><%= notice %></p>

<h1>Listing Slas</h1>

<table>
  <thead>
    <tr>
      <th>Sla1</th>
      <th>Sla2</th>
      <th>Diffnum</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @slas.each do |sla| %>
      <tr>
        <td><%= sla.sla1 %></td>
        <td><%= sla.sla2 %></td>
        <td><%= sla.diffnum %></td>
        <td><%= link_to 'Show', sla %></td>
        <td><%= link_to 'Edit', edit_sla_path(sla) %></td>
        <td><%= link_to 'Destroy', sla, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sla', new_sla_path %>

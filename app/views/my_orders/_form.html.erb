<%= simple_form_for(@my_order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :data1 %>
    <%= f.input :data2 %>
    <%= f.input :data3 %>
    <%= f.input :name %>
    <%= f.input :desc %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
